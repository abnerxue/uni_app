/* 导入图标 */
/* 提示 */
.tan{
	position: fixed;
	top: 60%;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	font-size: 24rpx;
	z-index: 99;
	background: rgba(46,46,46,0.8);
	padding: 20rpx 40rpx;
	color: white;
	border-radius:16rpx;
	max-width: 100%;
	white-space:nowrap;
}
/*设置状态栏 */
.status_bar {  
    height: var(--status-bar-height);  
    width: 100%;  
    background-color: #FFFFFF;
}
.top_view {  
    height: var(--status-bar-height);  
    width: 100%;  
    position: fixed;  
    background-color: #FFFFFF;  
    top: 0;  
    z-index: 999;
}
/* 顶部导航栏样式 */
.head {
	position: relative;
	height: 88rpx;
	width: 100%;
}
.heads {
	position: fixed;
	width: 100%;
	height: 88rpx;
	background: white;
	line-height: 90rpx;
	color: #1C0000;
	text-align: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 34rpx;
	font-weight: 600;
	z-index: 60;
	box-shadow: 0 3px 15px #F5F5F5;
	padding:0 30rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
/* 导航栏箭头 */
.arrowdown{
	margin-left: 10rpx;
	margin-top: 40rpx;
	width:0rpx;
	height:0rpx;
	border-width:14rpx 14rpx 0rpx;
	border-style:solid;
	border-color:#4A4A4A transparent transparent ;/*透明 透明  黑*/
}
.back {
	height: 100%;
	width: 100%;
	background: #F5F5F5;
	position: absolute;
}
/* 提示 */
.tishi {
	background: rgba(0, 0, 0, 0.6);
	width: 100%;
	height: 100%;
	position: fixed;
	z-index: 100;
	padding: 0 50rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.tishis {
	width: 100%;
	height: 210rpx;
	background: white;
	border-radius: 16rpx;
}
.tishis-content{
	width: 100%;
	height: 56%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	font-size: 28rpx;
	color: #1C0000;
	border-bottom: 1px solid #E9E9E9;
}
.tishi-btn{
	width: 100%;
	height: 44%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.quedin{
	height: 100%;
	width: 50%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	color: #74181B;
	font-size: 26rpx;
}
.quxiaos{
	height: 100%;
	width: 50%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
