
.res{
		position: relative;
		width: 100%;
		height: 90rpx;
		background: white;
}
.tabbar{
	position: fixed;
	width: 100%;
	height: 90rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 0 30rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	box-sizing: border-box;
	background: white;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	z-index: 50;
	color:#9A9A9A ;
	font-size:32rpx;
}
.icon-ldang{
	width: 30rpx;
	height: 33rpx;
}
.active{
	color: #74181B;
	font-size: 36rpx;
	border-bottom: 2px solid #74181B;
	font-weight:550;
	height:50rpx;
	padding: 0 10rpx;
}

