<view><view class="group_good_news"><view class="goods"><image class="goods_image" src="{{good.thumb}}"></image><view class="goods_news"><view class="good_title">{{good.title}}</view><view class="group_good_price"><text class="symbol">￥</text>{{group.price}}</view><view class="group_price"><text class="symbol">￥</text>{{good.price}}</view><block wx:if="{{group_news.group_initiate.status!=1}}"><image class="group_image_cs" src="{{group_news.group_initiate.status==2?'../../../static/group_success1.png':'../../../static/group_failure1.png'}}"></image></block></view></view></view><view class="group_news"><block wx:if="{{group_news.group_initiate.status==1}}"><view class="groups_ren">还差<text class="numbers">{{numbers+"人"}}</text>拼团成功，还剩<view class="example-body"><uni-countdown vue-id="1" minute="{{0}}" second="{{group_news.group_initiate.timeRemaining}}" show-colon="{{false}}" bind:__l="__l"></uni-countdown></view></view></block><block wx:if="{{group_news.group_initiate.status!=1}}"><view class="groups_ren"><image class="image_icon" src="{{group_news.group_initiate.status==2?'../../../static/group_success.png':'../../../static/group_failure.png'}}"></image><block wx:if="{{group_news.group_initiate.status==3}}"><text>恭喜您拼团成功</text></block><block wx:if="{{group_news.group_initiate.status==2}}"><text>拼团时间到</text></block></view></block><view class="group_image_list"><block wx:for="{{group_news.group_join}}" wx:for-item="group_image" wx:for-index="__i0__"><image class="group_image" src="{{group_image.avatar}}"></image></block><image class="group_image" src="../../../static/group/person.png"></image></view><block wx:if="{{group_chanjia==1}}"><button data-event-opts="{{[['tap',[['participate',['$event']]]]]}}" class="btn" bindtap="__e">我要参团</button></block><block wx:if="{{group_news.group_initiate.status==1&&group_chanjia!=1}}"><button class="btn">邀请好友</button></block><block wx:if="{{group_news.group_initiate.status!=1&&group_chanjia!=1}}"><button data-event-opts="{{[['tap',[['chakan',['$event']]]]]}}" class="btn" bindtap="__e">查看更多团购商品</button></block></view><view class="recommend_good"><view class="recommend_goods">推荐商品</view><view class="goodss"><block wx:for="{{goods}}" wx:for-item="goods" wx:for-index="__i1__"><view data-event-opts="{{[['tap',[['del',['$0'],[[['goods','',__i1__,'gid']]]]]]]}}" class="good" bindtap="__e"><image class="goods-images" src="{{goods.thumb}}"></image><view class="goods-title_price"><view class="good_titles">{{goods.title}}</view><view class="good_price">{{"￥"+goods.price}}</view></view></view></block></view></view></view>