<view><view class="status_bar"><view class="top_view"></view></view><view class="back"><view hidden="{{!(tan==true)}}" class="tan"><text>{{message}}</text></view><view class="head"><view class="heads"><view data-event-opts="{{[['tap',[['returns']]]]}}" bindtap="__e"><text class="iconfont icon-left"></text></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e">{{''+titles+''}}<text class="iconfont icon-sanjiao sanjiaos"></text></view><view></view></view></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" hidden="{{!(title_exhibit==true)}}" class="class_fy" bindtap="__e"><view class="class_fy_title"><block wx:for="{{first}}" wx:for-item="firsts" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['class_goods',['$0'],[[['first','',__i0__,'id']]]]]]]}}" class="{{[fids==firsts.id?'title_active':'title']}}" bindtap="__e">{{firsts.name}}</view></block></view></view><view class="tasb"><view class="tab"><view data-event-opts="{{[['tap',[['bidding']]]]}}" class="{{[tab==1?'act':'tabs']}}" bindtap="__e">竞价</view><view data-event-opts="{{[['tap',[['direct_purchase']]]]}}" class="{{[tab==2?'act':'tabs']}}" bindtap="__e">直购</view></view></view><view hidden="{{!(tab==2)}}" class="tabbar"><view class="tabbars"><view data-event-opts="{{[['tap',[['all']]]]}}" class="{{[choose==1?'screeningactive':'screening']}}" bindtap="__e">综合</view><view data-event-opts="{{[['tap',[['price_screening']]]]}}" hidden="{{!(tab==2)}}" class="{{[choose==2?'screeningactive':'screening']}}" bindtap="__e">价格</view><view data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" hidden="{{!(tab==2)}}" class="{{[choose==3?'screeningactive':'screening']}}" bindtap="__e">筛选</view></view></view><view hidden="{{!(options_title!=''||price_range!='')}}" class="tasb"><view class="tasbs"><view hidden="{{!(options_title!='')}}" class="options">{{options_title}}</view><view hidden="{{!(lowest_price!='')}}" class="options">{{lowest_price+"-"+highest_price}}</view></view></view><block wx:if="{{goods==null}}"><view class="wu">暂无商品</view></block><view class="goods"><block wx:for="{{goods}}" wx:for-item="good" wx:for-index="__i1__"><view data-event-opts="{{[['tap',[['del',['$0','$1'],[[['goods','',__i1__,'gid']],[['goods','',__i1__,'id']]]]]]]}}" class="goods_news" bindtap="__e"><image class="goods_image" src="{{good.thumb}}"></image><view class="goods_title">{{good.title}}</view><block wx:if="{{tab==2}}"><view class="goods_price">{{"￥"+good.price}}</view></block><block wx:if="{{tab==1}}"><view class="goods_price">{{"当前￥"+good.currentPrice}}</view></block></view></block></view><view hidden="{{!(screening==true)}}" class="screening_spring" animation="{{animationData}}" data-event-opts="{{[['tap',[['e3',['$event']]]]]}}" bindtap="__e"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="screen_spring" catchtap="__e"><view class="all"><view><view>全部类目</view></view><view class="option_all"><block wx:for="{{seconds}}" wx:for-item="seconds" wx:for-index="__i2__"><view data-event-opts="{{[['tap',[['secon',['$0'],[[['seconds','',__i2__,'id']]]]]]]}}" class="{{[seconds.id==categoryId?'option_active':'option']}}" bindtap="__e">{{seconds.name}}</view></block></view></view><view class="price"><view><view>价格区间</view></view><view class="price_all"><view class="highest_price"><input class="highest_prices" type="number" placeholder="最低价" data-event-opts="{{[['input',[['__set_model',['','lowest_price','$event',[]]]]]]}}" value="{{lowest_price}}" bindinput="__e"/></view><view class="pre">—</view><view class="highest_price"><input class="highest_prices" type="number" placeholder="最高价" data-event-opts="{{[['input',[['__set_model',['','highest_price','$event',[]]]]]]}}" value="{{highest_price}}" bindinput="__e"/></view></view></view><view class="btbs"><view class="btn-screen"><view class="reset_btn">重置</view><view data-event-opts="{{[['tap',[['screen',['$event']]]]]}}" class="are_btn" bindtap="__e">确定</view></view></view></view></view></view></view>