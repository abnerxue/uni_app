	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-left: 20rpx;
}
.goods_title {
	font-size: 28rpx;
	-webkit-line-clamp: 2;
	display: -webkit-box;
	word-wrap: break-word;
	word-break: normal;
	text-overflow: ellipsis;
	-webkit-box-orient: vertical;
	overflow: hidden;
}
.goods_price {
	color: #DD0812;
	font-size: 30rpx;
}
.goods_number {

	display: -webkit-box;

	display: -webkit-flex;

	display: flex;
