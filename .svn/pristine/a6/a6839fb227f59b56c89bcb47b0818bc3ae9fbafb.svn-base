<view><view class="status_bar"><view class="top_view"></view></view><view class="back"><view class="top_column"><view class="top_columns"><navigator open-type="navigateBack"><text class="iconfont icon-left"></text></navigator><view class="top_column_title">新增地址</view><view data-event-opts="{{[['tap',[['tianjia',['$event']]]]]}}" class="baocun" bindtap="__e"><text>保存</text></view></view></view><view hidden="{{!(tan)}}" class="tan"><text>{{message}}</text></view><view class="newlist"><view class="new_address"><view class="name"><text class="name-title">姓名</text><input class="input-address" type="text" placeholder="收货人姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view></view><view class="new_address"><view class="name"><text class="name-title">电话</text><input class="input-address" type="text" placeholder="收货人手机号" data-event-opts="{{[['input',[['__set_model',['','telephone','$event',[]]]]]]}}" value="{{telephone}}" bindinput="__e"/></view></view><view class="new_address"><view class="name"><text class="name-title">所在地区</text><view data-event-opts="{{[['tap',[['showCityPicker',['$event']]]]]}}" class="input-address" bindtap="__e">{{address2.key}}</view></view></view><view class="new_address"><view class="name"><text class="name-title">详细地址</text><input class="input-address" type="text" placeholder="街道门牌、楼层房间号等信息" data-event-opts="{{[['input',[['__set_model',['','address_del','$event',[]]]]]]}}" value="{{address_del}}" bindinput="__e"/></view></view><view class="new_address"><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" class="default" bindchange="__e"><radio style="transform:scale(0.6);" color="#DD0812" value="1"></radio><text class="name-title">设为默认</text></radio-group></view></view><mpvue-city-picker class="vue-ref" vue-id="1" themeColor="{{themeColor}}" pickerValueDefault="{{cityPickerValueDefault}}" data-ref="mpvueCityPicker" data-event-opts="{{[['^onCancel',[['onCanc