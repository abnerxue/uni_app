
.paper{
	background: #F1F1F1;
	height: 1334rpx;
}
/* 头部用户信息 */
.member-bg {
	width: 100%;
	height: 180rpx;
	background: #000000;
	position: relative;
}
.member-user {
	position: absolute;
	bottom: 30rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin: 0rpx 30rpx;
	width: 690rpx;
	height: 140rpx;
	background: #FBE7C4;
	border-radius: 16rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.member-user-image {
	margin-left: 20rpx;
	text-align: center;
	width: 80rpx;
	height: 80rpx;
}
.member-user-image image {
	width: 80rpx;
	height: 80rpx;
}
.usermessage-user {
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-left: 20rpx;
	font-size: 28rpx;
}
.user-level {
	margin-top: 10rpx;
	position: relative;
}
.user-level-img image {
	position: absolute;
	top: -1px;
	left: -3px;
	width: 42rpx;
	height: 42rpx;
}
.user-level-writing {
	/* align-items: center; */
	text-align: right;
	padding-right: 20rpx;
	width: 140rpx;
	height: 38rpx;
	line-height: 38rpx;
	background: #1C0000;
	font-size: 20rpx;
	border-radius: 20rpx;
	color: #FBE7C4;
}
/* 会员分类 */
.memberkinds{
	background: #FFFFFF;
	padding: 30rpx 32rpx 10rpx 32rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 690rpx;
	height: 250rpx;
	text-align: center;
}
.kind{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin: 0 6rpx;
	width: 220rpx;
	height: 250rpx;
	background: #FBE7C4;
	border-radius: 16rpx;
	border:1px solid sandybrown;
	color: #1C0000;
	font-size: 30rpx;
}
.memberkinds .active{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin: 0 6rpx;
	width: 220rpx;
	height: 250rpx;
	background: #1C0000;
	border-radius: 20rpx;
	border:1px solid #1C0000;
	color: #FBE7C4;
	font-size: 30rpx;
}
.kindcontent text{
	font-size: 48rpx;
}
/* 会员协议 */
.agreement{
	background: #FFFFFF;
	padding: 20rpx 30rpx;
	font-size: 28rpx;
}
.agreement text:first-child{
	color: #909399;
}
/* 选择支付方式 */
.content{
	width: 100%;
	height:340rpx;
	margin-top: 20rpx;
	/* background: white; */
}
.content-line{
	height: 1px;
	background: #eee;
}
.title {
	background: #FFFFFF;
	line-height: 50rpx;
	font-size: 28rpx;
	padding-left: 4%;
	padding-right: 4%;
	width: 92%;
}
.pay-list {
	background: white;
	width: 100%;
	border-bottom: solid 1rpx #eee;
}
.pay-list .row {
	padding-left: 4%;
	width: 92%;
	height: 120rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.pay-list .row .left {
	width: 80rpx;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.pay-list .row .left image {
	width: 60rpx;
	height: 60rpx;
}
.pay-list .row .center {
	width: 100%;
	font-size: 30rpx;
}
.pay-list .row .right {
	width: 100rpx;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
}
.pay-list .row .right radio{
	-webkit-transform: scale(0.8);
	        transform: scale(0.8);
}
.paymemberfoot{
	position: fixed;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 100rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	bottom: 0rpx;
	font-size: 30rpx;
}
.foot-left{
	background: #FFFFFF;
	width: 70%;
	height: 100rpx;
	text-align: right;
	padding-right: 20rpx;
}
.foot-left text{
	line-height: 100rpx;
	color:#DD0812 ;
}
.foot-right{
	text-align: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 30%;
	height: 100rpx;
	background: #F2DAA9;
}
.foot-right text{
	line-height: 100rpx;
}

